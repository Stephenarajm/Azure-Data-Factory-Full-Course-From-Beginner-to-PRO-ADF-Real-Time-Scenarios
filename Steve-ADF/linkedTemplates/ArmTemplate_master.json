{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Steve-ADF"
		},
		"ls_datalake_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_datalake'"
		},
		"ls_src1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_src1'"
		},
		"ls_tgt1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_tgt1'"
		},
		"RestService1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"ls_http_git_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"ls_tgt1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://velancovidreportingsa.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Steve-ADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_datalake_connectionString": {
						"value": "[parameters('ls_datalake_connectionString')]"
					},
					"ls_src1_connectionString": {
						"value": "[parameters('ls_src1_connectionString')]"
					},
					"ls_tgt1_accountKey": {
						"value": "[parameters('ls_tgt1_accountKey')]"
					},
					"RestService1_properties_typeProperties_url": {
						"value": "[parameters('RestService1_properties_typeProperties_url')]"
					},
					"ls_http_git_properties_typeProperties_url": {
						"value": "[parameters('ls_http_git_properties_typeProperties_url')]"
					},
					"ls_tgt1_properties_typeProperties_url": {
						"value": "[parameters('ls_tgt1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}